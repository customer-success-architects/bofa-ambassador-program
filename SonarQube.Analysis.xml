<?xml version="1.0" encoding="utf-8" ?>
<SonarQubeAnalysisProperties xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.sonarsource.com/msbuild/integration/2015/1">
    <!-- Connection Settings -->
    <Property Name="sonar.host.url">http://localhost:9000</Property>
    <Property Name="sonar.projectKey">abc</Property>

    <!-- Code Coverage Settings -->
    <Property Name="sonar.cs.cobertura.reportsPaths">DotnetApp.Tests/TestResults/**/coverage.cobertura.xml</Property>
    <Property Name="sonar.cs.opencover.reportsPaths">DotnetApp.Tests/TestResults/**/coverage.opencover.xml</Property>
    <Property Name="sonar.coverage.exclusions">**Test*.cs,**/*.Tests.cs</Property>

    <!-- Analysis Settings -->
    <Property Name="sonar.verbose">true</Property>
</SonarQubeAnalysisProperties>
